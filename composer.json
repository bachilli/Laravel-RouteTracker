{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "paquettg/php-html-parser": "^1.7",
        "guzzlehttp/guzzle": "^6.2",
        "laravelcollective/html": "@dev",
        "graham-campbell/htmlmin": "^4.3",
        "gsmeira/laravel-multialerts": "~v1.0.0",
        "gsmeira/laravel-tracker": "~v1.0.0",
        "gsmeira/laravel-assist": "dev-master@dev",
        "gsmeira/laravel-rulebuilder": "dev-master@dev",
        "gsmeira/laravel-thumbnail": "dev-master@dev",
        "gsmeira/laravel-filebox": "dev-master@dev",
        "gsmeira/laravel-metatags": "dev-master@dev",
        "gsmeira/laravel-prep": "dev-master@dev",
        "gsmeira/laravel-recaptcha": "dev-master@dev",
        "gsmeira/laravel-input": "dev-master@dev",
        "gsmeira/laravel-throttle": "dev-master@dev",
        "gsmeira/laravel-toggle": "dev-master@dev",
        "gsmeira/laravel-flag": "dev-master@dev",
        "gsmeira/laravel-uplab": "dev-master@dev",
        "gsmeira/laravel-clock": "dev-master@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-assist.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-rulebuilder.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-thumbnail.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-filebox.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-metatags.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-prep.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-recaptcha.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-input.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-throttle.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-toggle.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-flag.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-clock.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/laravel-uplab.git",
            "no-api": true
        }
    ]
}
