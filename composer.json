{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "paquettg/php-html-parser": "^1.7",
        "guzzlehttp/guzzle": "^6.2",
        "laravelcollective/html": "@dev",
        "graham-campbell/htmlmin": "^4.3",
        "gsmeira/laravel-multialerts": "dev-master@dev",
        "gsmeira/laravel-tracker": "dev-master@dev",
        "gsmeira/Laravel-assist": "dev-master@dev",
        "gsmeira/Laravel-rulebuilder": "dev-master@dev",
        "gsmeira/Laravel-thumbnail": "dev-master@dev",
        "gsmeira/Laravel-filebox": "dev-master@dev",
        "gsmeira/Laravel-metatags": "dev-master@dev",
        "gsmeira/Laravel-sysval": "dev-master@dev",
        "gsmeira/Laravel-recaptcha": "dev-master@dev",
        "gsmeira/Laravel-input": "dev-master@dev",
        "gsmeira/Laravel-throttle": "dev-master@dev",
        "gsmeira/Laravel-toggle": "dev-master@dev",
        "gsmeira/Laravel-humanval": "dev-master@dev",
        "gsmeira/Laravel-uplab": "dev-master@dev",
        "gsmeira/Laravel-clock": "dev-master@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Assist.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-RuleBuilder.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Thumbnail.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-FileBox.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-MetaTags.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-SysVal.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-reCAPTCHA.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Input.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Throttle.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Toggle.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-HumanVal.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-Clock.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:gsmeira/Laravel-UpLab.git",
            "no-api": true
        }
    ]
}