{
    "name": "gsmeira/laravel-routetracker",
    "type": "library",
    "description": "Extract information from the current route accessed.",
    "keywords": [
        "gsmeira",
        "laravel-routetracker",
        "laravel",
        "route",
        "tracker"
    ],
    "homepage": "https://github.com/gsmeira/Laravel-RouteTracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Meireles",
            "email": "gustavo@gsmeira.com",
            "homepage": "http://www.gsmeira.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "GSMeira\\LaravelRouteTracker\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GSMeira\\LaravelRouteTracker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
